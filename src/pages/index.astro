---
import { getEntry, render } from "astro:content";
import MarkdownLayout from "../layouts/MarkdownLayout.astro";

const indexPage = await getEntry('page', 'index');
if (!indexPage) {
  throw new Error("Index page not found");
}
const { Content } = await render(indexPage);
---

<MarkdownLayout title="首页">
	<Content />
</MarkdownLayout>